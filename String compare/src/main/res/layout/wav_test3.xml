<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="#000000"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/wavTest3ImageBtn5"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/wavTest3ImageBtn2"
        android:layout_above="@+id/wavTest3ImageBtn5"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/wavTest3ImageBtn8"
        android:layout_below="@+id/wavTest3ImageBtn5"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/wavTest3ImageBtn4"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@+id/wavTest3ImageBtn5" />

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/wavTest3ImageBtn6"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/wavTest3ImageBtn5" />

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/wavTest3ImageBtn1"
        android:layout_above="@+id/wavTest3ImageBtn5"
        android:layout_toLeftOf="@+id/wavTest3ImageBtn5" />

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/wavTest3ImageBtn3"
        android:layout_above="@+id/wavTest3ImageBtn5"
        android:layout_toRightOf="@+id/wavTest3ImageBtn5" />

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/wavTest3ImageBtn7"
        android:layout_below="@+id/wavTest3ImageBtn5"
        android:layout_toLeftOf="@+id/wavTest3ImageBtn5" />

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/wavTest3ImageBtn9"
        android:layout_below="@+id/wavTest3ImageBtn5"
        android:layout_toRightOf="@+id/wavTest3ImageBtn5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:text=""
        android:id="@+id/textView"
        android:layout_below="@+id/wavTest3ImageBtn8"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:text="@string/submit_button"
        android:textColor="#00FF0D"
        android:textSize="50sp"
        android:textStyle="bold"
        android:id="@+id/wavTest3SubmitBtn"
        android:layout_below="@+id/textView"
        android:layout_centerHorizontal="true" />
</RelativeLayout>